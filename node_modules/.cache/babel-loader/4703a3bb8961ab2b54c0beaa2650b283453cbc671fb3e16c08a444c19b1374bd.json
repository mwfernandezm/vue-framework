  })\r\n          .catch((error) => {\r\n            console.log(error);\r\n          });\r\n      }\r\n    },\r\n    nuevo() {\r\n      this.axios({\r\n        method: \"post\",\r\n        url: this.api + \"/activos\",\r\n        data: this.payload,\r\n      })\r\n        .then((response) => {\r\n          this.getList();\r\n          console.log(response.data);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n    getList() {\r\n      this.axios({\r\n        method: \"get\",\r\n        url: this.api + \"/activos\",\r\n      })\r\n        .then((response) => {\r\n          console.log(response.data);\r\n          this.items = response.data;\r\n          console.log(this.items);\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n    },\r\n  },\r\n  components: {},\r\n  mounted() {\r\n    this.getList();\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";AAqFA,eAAe;EACbA,IAAI,EAAE,YAAY;EAClBC,IAAIA,CAAA,EAAG;IACL,MAAMC,GAAE,GAAIC,OAAO,CAACC,GAAG,CAACC,WAAW;IACnC,OAAO;MACLH,GAAG;MACHI,KAAK,EAAE,EAAE;MACTC,MAAM,EAAE,EAAE;MACVC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;QACPC,IAAI,EAAE,IAAI;QACVC,KAAK,EAAE,IAAI;QACXC,MAAM,EAAE,IAAI;QACZC,MAAM,EAAE,IAAI;QACZC,MAAM,EAAE;MACV;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,MAAMA,CAACC,MAAM,EAAEC,KAAK,EAAE;MACpB,IAAI,CAACX,QAAO,GAAIW,KAAI,KAAM,EAAC,GAAI,EAAC,GAAI,GAAE,GAAID,MAAK,GAAI,GAAE,GAAIC,KAAK;MAC9D,IAAI,CAACC,OAAO,CAAC,CAAC;IAChB,CAAC;IACDC,MAAMA,CAACC,EAAE,EAAE;MACT,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,UAAS,GAAIF,EAAE,CAAC;IACpC,CAAC;IACDG,QAAQA,CAACH,EAAE,EAAE;MACX,IAAII,OAAO,CAAC,0BAA0B,CAAC,EAAE;QACvC,IAAI,CAACC,KAAK,CAAC;UACTC,MAAM,EAAE,QAAQ;UAChBC,GAAG,EAAE,IAAI,CAAC3B,GAAE,GAAI,WAAU,GAAIoB;QAChC,CAAC,EACEQ,IAAI,CAAEC,QAAQ,IAAK;UAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAC9B,IAAI,CAAC;UAC1B,IAAI,CAACmB,OAAO,CAAC,CAAC;QAChB,CAAC,EACAc,KAAK,CAAEC,KAAK,IAAK;UAChBH,OAAO,CAACC,GAAG,CAACE,KAAK,CAAC;QACpB,CAAC,CAAC;MACN;IACF,CAAC;IA