content\">\n        <form @submit.prevent=\"nuevo()\">\n          <h5>Nueva area</h5>\n          <p>Nombre: <input type=\"text\" v-model=\"payload.nombre\" required /></p>\n          <p>\n            Encargado:\n            <input type=\"text\" v-model=\"payload.encargado\" required />\n          </p>\n          <p>\n            Cantidad Funcionarios:\n            <input type=\"text\" v-model=\"payload.cantFuncionarios\" required />\n          </p>\n          <button type=\"submit\" class=\"waves-effect waves-light btn-small\">\n            Agregar\n          </button>\n        </form>\n      </div>\n    </div>\n    <div class=\"card\">\n      <div class=\"card-content\">\n        <form @submit.prevent=\"getList()\">\n          <h5>Buscar area</h5>\n          <p>\n            Nombre area:\n            <input type=\"search\" v-model=\"search\" @search=\"getList()\" />\n          </p>\n          <button type=\"submit\" class=\"waves-effect waves-light btn-small\">\n            buscar\n          </button>\n        </form>\n      </div>\n    </div>\n\n    <div class=\"card\">\n      <div class=\"card-content\">\n        <h5>Filtros</h5>\n        <div class=\"input-field\">\n          <select @change=\"filter('active', $event.target.value)\">\n            <option value=\"\" selected>todos</option>\n            <option value=\"true\">activo</option>\n            <option value=\"false\">inactivo</option>\n          </select>\n          <label>Materialize Select</label>\n        </div>\n      </div>\n    </div>\n\n    <table>\n      <thead>\n        <tr>\n          <th>Id</th>\n          <th>Nombre</th>\n          <th>Encargado</th>\n          <th>Cantidad Funcionarios</th>\n        </tr>\n      </thead>\n\n      <tbody>\n        <tr v-for=\"(item, index) in items\" :key=\"index\">\n          <td>{{ item.id }}</td>\n          <td>{{ item.nombre }}</td>\n          <td>{{ item.encargado }}</td>\n          <td>{{ item.cantFuncionarios }}</td>\n          <a class=\"btn-floating btn-large waves-effect waves-light red\"\n            ><i class=\"material-icons\" @click=\"eliminar(item.id)\">delete</i></a\n          >\n          <a class=\"btn-floating btn-large waves-effect waves-light blue\"\n            ><i class=\"material-icons\" @click=\"update(item.id)\">edit</i></a\n          >\n        </tr>\n      </tbody>\n    </table>\n  </div>\n</template>\n<script>\nexport default {\n  name: \"AreaView\",\n  data() {\n    const api = process.env.VUE_APP_API;\n    return {\n      api,\n      items: [],\n      payload: {\n        nombre: null,\n        encargado: null,\n        cantFuncionarios: null,\n      },\n    };\n  },\n  methods: {\n    update(id) {\n      this.$router.push(\"/area/\" + id);\n    },\n    eliminar(id) {\n      if (confirm(\"Esta seguro de eliminar?\")) {\n        this.axios({\n          method: \"delete\",\n          url: this.api + \"/areas/\" + id,\n        })\n          .then((response) => {\n            console.log(response.data);\n            this.getList();\n          })\n          .catch((error) => {\n            console.log(error);\n          });\n      }\n    },\n    nuevo() {\n      this.axios({\n        method: \"post\",\n        url: this.api + \"/areas\",\n        data: this.payload,\n      })\n        .then((response) => {\n          this.getList();\n          console.log(response.data);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n    getList() {\n      this.axios({\n        method: \"get\",\n        url: this.api + \"/areas\",\n      })\n        .then((response) => {\n          console.log(response.data);\n          this.items = response.data;\n          console.log(this.items);\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  components: {},\n  mounted() {\n    this.getList();\n  },\n};\n</script>\n"],"mappings":";AA+EA,eAAe;EACbA,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,MAAMC,GAAE,GAAIC,OAAO,CAACC,GAAG,CA